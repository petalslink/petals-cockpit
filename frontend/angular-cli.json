{
  "project": {
    "version": "1.0.0-beta.28.3",
    "name": "petals-cockpit"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "manifest.json"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "styles": [
        "../node_modules/material-design-icons/iconfont/material-icons.css",
        "styles.scss"
      ],
      "scripts": [
        "../node_modules/jdenticon/dist/jdenticon.min.js",
        "../node_modules/js-md5/build/md5.min.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts",
        "dev-e2e": "environments/environment.dev-e2e.ts",
        "dev-nomock": "environments/environment.dev-nomock.ts",
        "prod-e2e": "environments/environment.prod-e2e.ts",
        "gitlab-pages": "environments/environment.gitlab-pages.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "files": "src/**/*.ts",
      "project": "src/tsconfig.json"
    },
    {
      "files": "e2e/**/*.ts",
      "project": "e2e/tsconfig.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
