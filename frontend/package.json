{
  "name": "petals-cockpit",
  "version": "0.12.0-alpha",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "proxy": "ng serve --proxy-config proxy.conf.json -e=dev-nomock",
    "test": "ng test",
    "test:ci": "yarn run test -- --single-run --progress=false",
    "check": "yarn run lint:check && yarn run copyrights:check && yarn run prettier:check",
    "check:fix": "yarn run lint:fix && yarn run copyrights:fix && yarn run prettier:fix",
    "lint:check": "ng lint --type-check",
    "lint:fix": "yarn run lint:check -- --fix",
    "copyrights:fix": "node generate-headers.js",
    "copyrights:check": "yarn run copyrights:fix -- --dry",
    "e2e": "ng e2e -e=dev-e2e",
    "e2e:ci": "webdriver-manager update --versions.chrome=2.29 --gecko=false && ng e2e --webdriver-update false --prod --progress=false -e=prod-e2e",
    "e2e:product": "webdriver-manager update --versions.chrome=2.29 --gecko=false && ng e2e --webdriver-update false --serve=false --specs=e2e/login.e2e-spec.ts --specs=e2e/workspaces.e2e-spec.ts --specs=e2e/workspace-tree.e2e-spec.ts --specs=e2e/workspace-overview.e2e-spec.ts --specs=e2e/administration.e2e-spec.ts",
    "build": "ng build --prod",
    "build:product": "yarn run build -- -bh='{{baseUrl}}'",
    "build:product:ci": "yarn run build:product -- --progress=false",
    "build:demo": "yarn run build -- -e=gitlab-pages -bh=/petals-cockpit/",
    "build:demo:ci": "yarn run build:demo -- --progress=false",
    "build-electron-mock": "yarn run build -- --e=prod-e2e -bh='./'; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "electron-mock": "yarn run build-electron-mock; electron dist/",
    "build-electron": "yarn run build -- -bh='./'; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "electron": "yarn run build-electron; electron dist/",
    "prettier": "prettier",
    "prettier:base": "npm run prettier -- --single-quote --trailing-comma es5",
    "prettier:fix": "npm run prettier:base -- --write './{e2e,src}/**/*.{scss,ts}'",
    "prettier:check": "npm run prettier:fix -- -l",
    "precommit": "lint-staged",
    "prepush": "yarn run lint:check && yarn run copyrights:check"
  },
  "lint-staged": {
    "gitDir": "../",
    "linters": {
      "*.ts": [
        "npm run prettier:base -- -l"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.1.3",
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/material": "2.0.0-beta.7",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/router": "4.1.3",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.4",
    "@ngrx/store": "2.2.3",
    "@ngrx/store-devtools": "3.2.4",
    "angular2-notifications": "0.7.4",
    "angular2-prettyjson": "2.0.5",
    "angular2-useful-swiper": "4.0.5",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "jdenticon": "1.6.0",
    "js-md5": "0.4.2",
    "lodash": "4.17.4",
    "markdown-to-html-pipe": "1.2.1",
    "material-design-icons-iconfont": "3.0.3",
    "ng2-translate": "5.0.0",
    "ng2-vis": "0.0.6",
    "ngrx-store-freeze": "0.1.9",
    "ngx-pipes": "1.6.1",
    "rxjs": "5.4.2",
    "swiper": "3.4.2",
    "ts-helpers": "1.1.2",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "1.2.0",
    "@angular/compiler-cli": "4.1.3",
    "@angular/language-service": "4.1.3",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "2.0.2",
    "@types/lodash": "4.14.68",
    "@types/node": "8.0.10",
    "codelyzer": "3.1.2",
    "husky": "0.14.3",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "lint-staged": "4.0.1",
    "prettier": "1.5.2",
    "protractor": "5.1.2",
    "ts-node": "3.2.0",
    "tslint": "5.5.0",
    "tslint-config-prettier": "1.1.0",
    "tslint-language-service": "0.9.6",
    "typescript": "2.4.1"
  }
}
