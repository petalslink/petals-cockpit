{
  "name": "petals-cockpit",
  "version": "0.12.0-alpha",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "proxy": "ng serve --proxy-config proxy.conf.json -e=dev-nomock",
    "test": "ng test",
    "test:ci": "yarn run test --single-run --progress=false",
    "check": "yarn run tslint-config-prettier-check ./tslint.json && yarn run lint:check && yarn run copyrights:check && yarn run prettier:check",
    "check:fix": "yarn run lint:fix; yarn run copyrights:fix; yarn run prettier:fix",
    "lint:check": "ng lint --type-check",
    "lint:fix": "yarn run lint:check --fix",
    "copyrights:fix": "node generate-headers.js",
    "copyrights:check": "yarn run copyrights:fix --dry",
    "e2e": "ng e2e -e=dev-e2e",
    "e2e:ci": "ng e2e --prod --progress=false -e=prod-e2e",
    "e2e:product": "ng e2e --serve=false --specs=e2e/login.e2e-spec.ts --specs=e2e/workspaces.e2e-spec.ts --specs=e2e/workspace-tree.e2e-spec.ts --specs=e2e/workspace-overview.e2e-spec.ts --specs=e2e/administration.e2e-spec.ts",
    "build": "ng build --prod",
    "build:product": "yarn run build -bh='{{baseUrl}}'",
    "build:product:ci": "yarn run build:product --progress=false",
    "build:demo": "yarn run build -e=gitlab-pages -bh=/petals-cockpit/",
    "build:demo:ci": "yarn run build:demo --progress=false",
    "build-electron-mock": "yarn run build --e=prod-e2e -bh='./'; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "electron-mock": "yarn run build-electron-mock; electron dist/",
    "build-electron": "yarn run build -bh='./'; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "postbuild": "gulp compress",
    "electron": "yarn run build-electron; electron dist/",
    "prettier:base": "yarn run prettier --single-quote --trailing-comma es5",
    "prettier:base-files": "yarn run prettier:base './{e2e,src}/**/*.{scss,ts}'",
    "prettier:fix": "yarn run prettier:base-files --write",
    "prettier:check": "yarn run prettier:base-files -l",
    "precommit": "lint-staged",
    "prepush": "yarn run lint:check && yarn run copyrights:check"
  },
  "lint-staged": {
    "gitDir": "../",
    "linters": {
      "*.{ts,scss}": [
        "yarn run prettier:base -l"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.3.6",
    "@angular/cdk": "2.0.0-beta.10",
    "@angular/common": "4.3.6",
    "@angular/compiler": "4.3.6",
    "@angular/core": "4.3.6",
    "@angular/flex-layout": "2.0.0-beta.9",
    "@angular/forms": "4.3.6",
    "@angular/http": "4.3.6",
    "@angular/material": "2.0.0-beta.10",
    "@angular/platform-browser": "4.3.6",
    "@angular/platform-browser-dynamic": "4.3.6",
    "@angular/router": "4.3.6",
    "@ngrx/effects": "4.0.5",
    "@ngrx/store": "4.0.3",
    "@ngrx/store-devtools": "4.0.0",
    "angular2-notifications": "0.7.7",
    "angular2-prettyjson": "2.0.5",
    "angular2-useful-swiper": "5.0.0",
    "core-js": "2.5.1",
    "hammerjs": "2.0.8",
    "jdenticon": "1.7.2",
    "js-md5": "0.6.0",
    "lodash": "4.17.4",
    "markdown-to-html-pipe": "1.2.2",
    "marked": "0.3.6",
    "material-design-icons-iconfont": "3.0.3",
    "ng2-vis": "0.0.6",
    "ngrx-store-freeze": "0.1.9",
    "ngx-hover-opacity": "0.2.0",
    "ngx-pipes": "1.6.4",
    "rxjs": "5.4.3",
    "swiper": "4.0.0-beta.2",
    "ts-helpers": "1.1.2",
    "uuid": "3.1.0",
    "zone.js": "0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "1.4.0",
    "@angular/compiler-cli": "4.3.6",
    "@angular/language-service": "4.3.6",
    "@types/jasmine": "2.5.54",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "4.14.74",
    "@types/node": "8.0.27",
    "codelyzer": "3.1.2",
    "gulp": "3.9.1",
    "gulp-gzip": "1.4.0",
    "husky": "0.14.3",
    "jasmine-core": "2.8.0",
    "jasmine-marbles": "0.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-scss-preprocessor": "3.0.0",
    "lint-staged": "4.1.3",
    "prettier": "1.7.0",
    "protractor": "5.1.2",
    "ts-node": "3.3.0",
    "tslint": "5.7.0",
    "tslint-config-prettier": "1.5.0",
    "tslint-language-service": "0.9.6",
    "typescript": "2.5.2"
  }
}
