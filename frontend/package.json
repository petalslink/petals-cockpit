{
  "name": "petals-cockpit",
  "version": "0.4.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "precache": "sw-precache --verbose --config=sw-precache-config.js",
    "build": "ng build --prod --aot --no-sourcemap",
    "prod": "yarn run build && yarn run precache",
    "prod-mock": "yarn run build --e=prod-e2e && yarn run precache",
    "build-electron-mock": "yarn run build --e=prod-e2e -bh='./' && yarn run precache; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "electron-mock": "yarn run build-electron-mock; electron dist/",
    "build-electron": "yarn run build -bh='./' && yarn run precache; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "electron": "yarn run build-electron; electron dist/"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.4",
    "@angular/compiler": "2.4.4",
    "@angular/core": "2.4.4",
    "@angular/flex-layout": "2.0.0-beta.1",
    "@angular/forms": "2.4.4",
    "@angular/http": "2.4.4",
    "@angular/material": "2.0.0-beta.1",
    "@angular/platform-browser": "2.4.4",
    "@angular/platform-browser-dynamic": "2.4.4",
    "@angular/router": "3.4.4",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "3.2.3",
    "angular2-notifications": "0.4.49",
    "angular2-prettyjson": "2.0.2",
    "chance": "1.0.4",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "jdenticon": "1.4.0",
    "js-md5": "0.4.2",
    "material-design-icons": "3.0.1",
    "ng2-translate": "5.0.0",
    "ngrx-store-freeze": "0.1.6",
    "redux-batched-actions": "0.1.5",
    "rxjs": "5.0.3",
    "ts-helpers": "1.1.2",
    "underscore": "1.8.3",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.4",
    "@types/chance": "0.7.31",
    "@types/jasmine": "2.5.41",
    "@types/node": "7.0.0",
    "@types/underscore": "1.7.36",
    "angular-cli": "1.0.0-beta.26",
    "codelyzer": "2.0.0-beta.4",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-clear-screen-reporter": "1.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-diff-reporter": "0.6.3",
    "karma-remap-istanbul": "0.4.0",
    "protractor": "5.0.0",
    "sw-precache": "4.3.0",
    "ts-node": "2.0.0",
    "tslint": "4.3.1",
    "typescript": "2.0.3"
  }
}
