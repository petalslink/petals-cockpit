{
  "name": "petals-cockpit",
  "version": "0.5.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json -e=dev-nomock",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --prod --aot --no-sourcemap",
    "build-silent": "yarn run build -- --progress=false",
    "prod": "yarn run build -- -bh='{{baseUrl}}'",
    "prod-silent": "yarn run build-silent -- -bh='{{baseUrl}}'",
    "prod-mock": "yarn run build -- --e=prod-e2e",
    "prod-demo": "yarn run build-silent -- -e=gitlab-pages -bh=/petals-cockpit/",
    "build-electron-mock": "yarn run build -- --e=prod-e2e -bh='./'; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "electron-mock": "yarn run build-electron-mock; electron dist/",
    "build-electron": "yarn run build -- -bh='./'; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "electron": "yarn run build-electron; electron dist/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.0.0-rc.2",
    "@angular/common": "4.0.0-rc.2",
    "@angular/compiler": "4.0.0-rc.2",
    "@angular/core": "4.0.0-rc.2",
    "@angular/flex-layout": "2.0.0-rc.1",
    "@angular/forms": "4.0.0-rc.2",
    "@angular/http": "4.0.0-rc.2",
    "@angular/material": "2.0.0-beta.2",
    "@angular/platform-browser": "4.0.0-rc.2",
    "@angular/platform-browser-dynamic": "4.0.0-rc.2",
    "@angular/router": "4.0.0-rc.2",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "3.2.3",
    "angular2-notifications": "0.4.53",
    "angular2-prettyjson": "2.0.3",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "jdenticon": "1.4.0",
    "js-md5": "0.4.2",
    "material-design-icons-iconfont": "3.0.3",
    "ng2-translate": "5.0.0",
    "ngrx-store-freeze": "0.1.9",
    "redux-batched-actions": "0.1.5",
    "rxjs": "5.2.0",
    "ts-helpers": "1.1.2",
    "underscore": "1.8.3",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-rc.1",
    "@angular/compiler-cli": "4.0.0-rc.2",
    "@types/jasmine": "2.5.43",
    "@types/node": "7.0.5",
    "@types/underscore": "1.7.36",
    "codelyzer": "2.0.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-clear-screen-reporter": "1.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.3",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-diff-reporter": "1.0.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.1.1",
