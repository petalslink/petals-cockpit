{
  "name": "petals-cockpit",
  "version": "0.4.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json -e=dev-nomock",
    "precache": "yarn run precache-silent -- --verbose",
    "precache-silent": "sw-precache --config=sw-precache-config.js",
    "build": "ng build --prod --aot --no-sourcemap",
    "build-silent": "yarn run build -- --progress=false",
    "prod": "yarn run build && yarn run precache",
    "prod-silent": "yarn run build-silent && yarn run precache-silent",
    "prod-mock": "yarn run build --e=prod-e2e && yarn run precache",
    "build-electron-mock": "yarn run build --e=prod-e2e -bh='./' && yarn run precache; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "electron-mock": "yarn run build-electron-mock; electron dist/",
    "build-electron": "yarn run build -bh='./' && yarn run precache; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "electron": "yarn run build-electron; electron dist/"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.4.7",
    "@angular/compiler": "2.4.7",
    "@angular/core": "2.4.7",
    "@angular/flex-layout": "2.0.0-beta.5",
    "@angular/forms": "2.4.7",
    "@angular/http": "2.4.7",
    "@angular/material": "https://github.com/angular/material2-builds.git#53fae1576e981033505947c6f63032d147350e25",
    "@angular/platform-browser": "2.4.7",
    "@angular/platform-browser-dynamic": "2.4.7",
    "@angular/router": "3.4.7",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "3.2.3",
    "angular2-notifications": "0.4.53",
    "angular2-prettyjson": "2.0.3",
    "chance": "1.0.4",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "jdenticon": "1.4.0",
    "js-md5": "0.4.2",
    "material-design-icons": "3.0.1",
    "ng2-translate": "5.0.0",
    "ngrx-store-freeze": "0.1.6",
    "redux-batched-actions": "0.1.5",
    "rxjs": "5.1.0",
    "ts-helpers": "1.1.2",
    "underscore": "1.8.3",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.31",
    "@angular/compiler-cli": "2.4.7",
    "@types/chance": "0.7.31",
    "@types/jasmine": "2.5.41",
    "@types/node": "7.0.5",
    "@types/underscore": "1.7.36",
    "codelyzer": "2.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-clear-screen-reporter": "1.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-diff-reporter": "1.0.0",
