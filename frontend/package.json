{
  "name": "petals-cockpit",
  "version": "0.7.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "proxy": "ng serve --proxy-config proxy.conf.json -e=dev-nomock",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "check": "yarn run check:lint && yarn run check:copyrights",
    "check:lint": "ng lint",
    "check:copyrights": "node generate-headers.js --dry",
    "e2e": "ng e2e -e=dev-e2e",
    "e2e:ci": "ng e2e --prod --progress=false -e=prod-e2e",
    "e2e:product": "ng e2e --serve=false --specs=e2e/login.e2e-spec.ts --specs=e2e/workspaces.e2e-spec.ts --specs=e2e/workspace-tree.e2e-spec.ts --specs=e2e/workspace-overview.e2e-spec.ts",
    "build": "ng build --prod --aot --no-sourcemap",
    "build-silent": "yarn run build -- --progress=false",
    "prod": "yarn run build -- -bh='{{baseUrl}}'",
    "prod-silent": "yarn run build-silent -- -bh='{{baseUrl}}'",
    "prod-mock": "yarn run build -- --e=prod-e2e",
    "prod-demo": "yarn run build-silent -- -e=gitlab-pages -bh=/petals-cockpit/",
    "build-electron-mock": "yarn run build -- --e=prod-e2e -bh='./'; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "electron-mock": "yarn run build-electron-mock; electron dist/",
    "build-electron": "yarn run build -- -bh='./'; node ./electron/generate-package-json.js; cp ./electron/electron.js dist/",
    "electron": "yarn run build-electron; electron dist/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.0.1",
    "@angular/common": "4.0.1",
    "@angular/compiler": "4.0.1",
    "@angular/core": "4.0.1",
    "@angular/flex-layout": "2.0.0-rc.1",
    "@angular/forms": "4.0.1",
    "@angular/http": "4.0.1",
    "@angular/material": "2.0.0-beta.2",
    "@angular/platform-browser": "4.0.1",
    "@angular/platform-browser-dynamic": "4.0.1",
    "@angular/router": "4.0.1",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.2",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "3.2.4",
    "angular2-notifications": "0.4.53",
    "angular2-prettyjson": "2.0.5",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "jdenticon": "1.4.0",
    "js-md5": "0.4.2",
    "markdown-to-html-pipe": "1.2.0",
    "material-design-icons-iconfont": "3.0.3",
    "ng2-translate": "5.0.0",
    "ngrx-store-freeze": "0.1.9",
    "rxjs": "5.3.0",
    "ts-helpers": "1.1.2",
    "underscore": "1.8.3",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "4.0.1",
    "@types/jasmine": "2.5.45",
    "@types/jest": "19.2.2",
    "@types/node": "6.0.66",
    "@types/underscore": "1.8.0",
    "codelyzer": "2.0.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "jest": "19.0.2",
    "jest-preset-angular": "1.0.0",
    "protractor": "5.1.1",
    "ts-node": "3.0.2",
    "tslint": "4.5.1",
    "typescript": "2.2.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup-jest.ts",
    "collectCoverageFrom": ["src/**/*.ts", "!src/**/*.spec.ts", "!src/environment/**", "!src/**/*.mock.ts", "!src/mocks/**", "!src/**/*.d.ts"],
    "coveragePathIgnorePatterns": ["<rootDir>/src/environment", "<rootDir>/src/mocks"]
  }
}
